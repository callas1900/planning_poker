    "eslint-loader": "^3.0.2",
    "jest": "^26.0.1",
    "jest-puppeteer": "^4.4.0",
    "node-sass": "^4.5.3",
    "puppeteer": "^3.1.0",
    "sass-loader": "^6.0.6",
